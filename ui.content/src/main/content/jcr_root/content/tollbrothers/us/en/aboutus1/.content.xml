                        jcr:lastModified="{Date}2023-11-03T20:13:55.964+05:30"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/textimage"
                        text="&lt;p>&lt;b>CROSSING THE BORDER&lt;/b>&lt;/p>&#xa;&lt;p>Toll Brothers makes its first move out of Pennsylvania, entering neighboring state New Jersey.&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true">
                        <image
                            jcr:lastModified="{Date}2023-11-03T18:09:10.871+05:30"
