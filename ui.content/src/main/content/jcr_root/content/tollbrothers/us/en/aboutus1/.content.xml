                    <textimage_718570918
                        jcr:created="{Date}2023-11-03T18:10:24.264+05:30"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2023-11-03T20:11:25.981+05:30"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/textimage"
                        text="&lt;p>&lt;b>GOING PUBLIC&lt;/b>&lt;/p>&#xa;&lt;p>Toll Brothers becomes a public company, and is listed on the New York Stock Exchange (NYSE: TOL) with revenues of $124.6 million from 15 communities.&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"
                        textIsRich="true">
